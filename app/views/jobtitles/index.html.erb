<h1>Listing jobtitles</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @jobtitles.each do |jobtitle| %>
      <tr>
        <td><%= jobtitle.description %></td>
        <td><%= link_to 'Show', jobtitle %></td>
        <td><%= link_to 'Edit', edit_jobtitle_path(jobtitle) %></td>
        <td><%= link_to 'Destroy', jobtitle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobtitle', new_jobtitle_path %>
